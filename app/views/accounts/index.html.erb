<%= render 'shared/panel', title: 'Accounts', data_test: 'staff-panel' do %>
  <div class="list-group">
    <% @accounts.each do |account| %>
      <%= link_to "#{account.name}", account_path(account), class: "list-group-item", data: { test: "account-link" } %>
    <% end %>
  </div>
  <%= link_to 'Add Account', new_account_path, class: 'btn btn-primary', data: { test: 'new-account-button' } %>
<% end %>
