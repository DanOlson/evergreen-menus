<div class='container'>
  <div class='page-heading clearfix'>
    <div class='pull-left establishment-name'>{{name}}</div><div class='pull-right address'>{{address}}</div>
  </div>
  <div class='col-md-4'>
    <h3>Beer List</h3>
    <ul>
      {{#each beer in beers}}
        <li>{{beer.name}}</li>
      {{/each}}
    </ul>
  </div>
  <div class='gmap col-md-4'>{{view Beermapper.MapView}}</div>
</div>
